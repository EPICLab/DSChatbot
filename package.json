{
  "name": "anachat",
  "version": "0.1.0",
  "description": "Chatbot for supporting data science analyses in Jupyter Lab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/EPICLab/DSChatbot",
  "bugs": {
    "url": "https://github.com/EPICLab/DSChatbot/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Joao Felipe Pimentel",
    "email": "joaofelipenp@gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/EPICLab/DSChatbot.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:prod": "jlpm clean && npm install && tsc && webpack --mode=production && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc && webpack --mode=development",
    "clean": "jlpm clean:lib clean:labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf anachat/labextension",
    "clean:rest": "rimraf anachat.egg-info node_modules dist",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache && jlpm clean:rest",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "npm install && jlpm build",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "webpack --watch --mode=development",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^3.5.0",
    "@jupyterlab/apputils": "3.5.0",
    "@jupyterlab/cells": "3.5.0",
    "@jupyterlab/coreutils": "^5.5.0",
    "@jupyterlab/docmanager": "3.5.0",
    "@jupyterlab/notebook": "3.5.0",
    "@jupyterlab/observables": "4.5.0",
    "@jupyterlab/rendermime": "3.5.0",
    "@jupyterlab/services": "6.5.0",
    "@jupyterlab/statusbar": "3.5.0",
    "@jupyterlab/translation": "3.5.0",
    "@jupyterlab/ui-components": "3.5.0",
    "@lumino/algorithm": "1.9.2",
    "@lumino/commands": "1.21.0",
    "@lumino/coreutils": "1.12.1",
    "@lumino/disposable": "1.10.3",
    "@lumino/messaging": "1.10.3",
    "@lumino/signaling": "1.11.0",
    "@lumino/widgets": "1.35.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@jupyterlab/builder": "^3.3.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/lodash": "^4.14.182",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "~25.0.0",
    "stylelint-prettier": "^2.0.0",
    "svelte": "^3.46.6",
    "svelte-collapsible": "^0.2.1",
    "svelte-loader": "^3.1.2",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-preprocess": "^4.10.5",
    "svg-url-loader": "^7.1.1",
    "tailwindcss": "^3.0.24",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "anachat"
        }
      }
    },
    "extension": true,
    "outputDir": "anachat/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/application": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/apputils": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/attachments": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/cells": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/coreutils": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/notebook": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/rendermine": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/services": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/shared-models": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/statusbar": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/translation": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/ui-components": {
        "bundled": false,
        "singleton": true
      },
      "@lumino/widgets": {
        "bundled": false,
        "singleton": true
      },
      "@lumino/commands": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.1",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
